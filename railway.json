{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd jules-backend && npm install",
    "watchPatterns": [
      "jules-backend/**"
    ]
  },
  "deploy": {
    "startCommand": "cd jules-backend && npm start",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
} 